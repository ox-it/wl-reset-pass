  <artifactId>sakai-reset-pass</artifactId>
  <organization>
    <name>Sakai Foundation</name>
    <url>http://sakaiproject.org/</url>
  </organization>
  <inceptionYear>2006</inceptionYear>
  <packaging>war</packaging>
  <dependencies>
  
  <!-- RSF Dependencies -->
     <dependency>
